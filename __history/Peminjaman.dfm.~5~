  object QueryPeminjaman: TZQuery
    Connection = PeminjamanCon
    Active = True
    SQL.Strings = (
      'SELECT'
      '    peminjaman.id_peminjaman,'
      '    member.name AS member_name,'
      '    buku.title AS book_title'
      'FROM'
      '    peminjaman'
      'JOIN'
      '    member ON peminjaman.member_id = member.id_member'
      'JOIN'
      '    buku ON peminjaman.book_id = buku.id_buku')
    Params = <>
    Left = 448
    Top = 144
